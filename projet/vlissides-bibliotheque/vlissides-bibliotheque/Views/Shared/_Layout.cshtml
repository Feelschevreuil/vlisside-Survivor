<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - vlissides_bibliotheque</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/icon.css" asp-append-version="true" /> 
    <link rel="stylesheet" href="~/css/GestionCours.css" asp-append-version="true" />
    <!--link rel="stylesheet" href="~/vlissides_bibliotheque.styles.css" asp-append-version="true" /-->
    <link rel="stylesheet" href="~/css/tableau-de-bord.css" asp-append-version="true" />
</head>
<body id="PageCourante">

    <partial name="_BarreDeNaviguationPartial">
        

    <div class="container">
        <main role="main" class="p-3">
            @RenderBody()
        </main>
    </div>

    <partial name="_footerPartial">
        
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/LivreDetail.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/modifierCreerLivre.js" asp-append-version="true"></script>
    <script src="~/js/file-to-base64.js" asp-append-version="true"></script>
    <script src="~/js/navbar.js" asp-append-version="true"></script>
    <script src="~/js/froms.js" asp-append-version="true"></script>
    <script src="~/js/panierItems.js" asp-append-version="true"></script>
    <script src="~/js/gestionCours.js" asp-append-version="true"></script>
    <script src="~/js/QuantitePanier.js" asp-append-version="true"></script>
    <script src="~/js/changerDePage.js" asp-append-version="true"></script>
    <script src="~/js/documentation.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
