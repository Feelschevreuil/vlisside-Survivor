@model List<LivreEtudiant>


<div class="container-fluid">
    <div class="row">
        @if (Model.Count() > 0 && Model != null)
        {
            <h1>Les livres suivants ont été associer à un étudiant</h1>
            @foreach (var livre in Model)
            {
                <p><strong>@livre.Etudiant.Nom</strong></p>
                <p>@livre.Titre</p>
            }
        }
        else
        {
            <h1>Aucun livre n'a été ajouter</h1>
        }
    </div>
</div>


<link rel="stylesheet" href="~/css/carte-livre-Partiel.css" asp-append-version="true" />
<script src="~/js/updateBoutonAjoutPanier.js"></script>
<script src="~/js/PrixAjax.js"></script>